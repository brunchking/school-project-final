        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.10.3/jquery-ui.js"></script>
    <!--DON'T TOUCH!!! jquery, bootstrap,script-->

    <!--left side nav-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.10.3/jquery-ui.js"></script>

    <!--MY CSS-->
    <link rel="stylesheet" href="css/nav.css">
    <link rel="stylesheet" href="css/socialMediaSideBar.css">
    <link rel="stylesheet" href="css/community.css">
    <!--CSS-->

    <style>
    </style>
</head>

<body>
    <!--animated nav-->
    <%- include('partials/nav') %>

    <!--social sidebar-->
    <% include partials/social-sidebar %>

    <!-- upload -->
    <% if (isLogin === true) { %>
    <div class="container community">
        <div class="upload-pic">
            <form action="/community" enctype="multipart/form-data" method="post">
                <input type="file" name='image'>
                <input type="submit" value="submit">
            </form>
            <br>
            <%= typeof msg != 'undefined' ? msg : '' %>
            <div class="display-pic">
                <img id="image" src="<%= typeof filename != 'undefined' ? filename: '' %>" class="responsive-img">
            </div>
        </div>
    </div>
    <% } %>

    <% if (isLogin === false) { %> 
        <div class="pillar">

        </div>
    <% } %>
    <!-- footer -->
    <% include partials/footer %>

    <!--myJavaScript-->
    <script src="../js/navNoEffect.js"></script>
</body>

</html>